        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2209F2_wpf"
        mc:Ignorable="d"
        Title="dataForm" Height="450" Width="800">
    <Grid>
        <Label x:Name="data" Content="Label" HorizontalAlignment="Left" Margin="212,32,0,0" VerticalAlignment="Top"/>
        <DataGrid x:Name="dataGrid" d:ItemsSource="{d:SampleData ItemCount=5}"/>

    </Grid>
</Window>
